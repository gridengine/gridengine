    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="detailed_job_info">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>qstat -j number output</xs:documentation>
			</xs:annotation>
			<xs:sequence >
				<xs:element name="djob_info" type="djob_info_t"/>
				<xs:element name="messages" type="message_t"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:complexType name="djob_info_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
		       <xs:element name="qmaster_response" type="qmaster_response_t"/>
	    </xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="qmaster_response_t">
		<xs:sequence >
                <xs:element name="JB_job_number" type="xs:unsignedInt"/>
                <xs:element name="JB_job_name" type="xs:string"/>
                <xs:element name="JB_version" type="xs:unsignedInt"/>
                <xs:element name="JB_session" type="xs:string"/>
                <xs:element name="JB_department" type="xs:string"/>
                <xs:element name="JB_exec_file" type="xs:string"/>
                <xs:element name="JB_script_file" type="xs:string"/>
                <xs:element name="JB_script_size" type="xs:unsignedInt"/>
                <xs:element name="JB_submission_time" type="xs:unsignedInt"/>
                <xs:element name="JB_execution_time" type="xs:unsignedInt"/>
                <xs:element name="JB_deadline" type="xs:unsignedInt"/>
                <xs:element name="JB_jid_request_list" type="JB_jid_request_list_t" minOccurs="0"/>
                <xs:element name="JB_jid_sucessor_list" type="JB_jid_sucessor_list_t" minOccurs="0"/>
                <xs:element name="JB_owner" type="xs:string"/>
                <xs:element name="JB_uid" type="xs:string"/>
                <xs:element name="JB_group" type="xs:string"/>
                <xs:element name="JB_gid" type="xs:unsignedInt"/>
                <xs:element name="JB_account" type="xs:string"/>
                <xs:element name="JB_notify" type="xs:boolean"/>
                <xs:element name="JB_type" type="xs:unsignedInt"/>
                <xs:element name="JB_reserve" type="xs:boolean"/>
                <xs:element name="JB_priority" type="xs:unsignedInt"/>
                <xs:element name="JB_jobshare" type="xs:unsignedInt"/>
                <xs:element name="JB_shell_list" type="JB_shell_list_t"/>
                <xs:element name="JB_verify" type="xs:boolean" minOccurs="0" />
                <xs:element name="JB_env_list" type="JB_env_list_t"/>
                <xs:element name="JB_checkpoint_attr" type="xs:unsignedInt"/>
                <xs:element name="JB_checkpoint_object" type="xs:string" minOccurs="0"/>
                <xs:element name="JB_checkpoint_interval" type="xs:unsignedInt"/>
                <xs:element name="JB_restart" type="xs:unsignedInt"/>
                <xs:element name="JB_merge_stderr" type="xs:boolean"/>
                <xs:element name="JB_hard_queue_list" type="JB_hard_queue_list_t" minOccurs="0"/>
                <xs:element name="JB_mail_options" type="xs:unsignedInt" />
                <xs:element name="JB_mail_list" type="JB_mail_t"/>
                <xs:element name="JB_ja_structure" type="JB_ja_structure_t"/>
                <xs:element name="JB_ja_n_h_ids" type="JB_ja_structure_t" minOccurs="0"/>
                <xs:element name="JB_ja_u_h_ids" type="JB_ja_structure_t" minOccurs="0"/>
                <xs:element name="JB_ja_s_h_ids" type="JB_ja_structure_t" minOccurs="0"/>
                <xs:element name="JB_ja_o_h_ids" type="JB_ja_structure_t" minOccurs="0"/>
                <xs:element name="JB_ja_z_ids" type="JB_hja_structure_t" minOccurs="0"/>
                <xs:element name="JB_ja_template" type="ja_task_t"/>
                <xs:element name="JB_ja_tasks" type="ja_task_t"/>
                <xs:element name="JB_host" type="xs:string"/>
                <xs:element name="JB_verify_suitable_queues" type="xs:unsignedInt"/>
                <xs:element name="JB_nrunning" type="xs:unsignedInt"/>
                <xs:element name="JB_soft_wallclock_gmt" type="xs:unsignedInt"/>
                <xs:element name="JB_hard_wallclock_gmt" type="xs:unsignedInt"/>
                <xs:element name="JB_override_tickets" type="xs:unsignedInt"/>
                <xs:element name="JB_urg" type="xs:float"/>
                <xs:element name="JB_nurg" type="xs:float"/>
                <xs:element name="JB_nppri" type="xs:float"/>
                <xs:element name="JB_rrcontr" type="xs:float" minOccurs="0"/>
                
                <xs:element name="JB_dlcontr" type="xs:float" minOccurs="0"/>
                <xs:element name="JB_wtcontr" type="xs:float" minOccurs="0"/>		
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JB_jid_request_list_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="string_ublist" type="string_ublist_t"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="string_ublist_t">
		<xs:sequence>
			<xs:element name="JRE_job_number" type="xs:unsignedInt"/>
			<xs:element name="JRE_job_name" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JB_jid_sucessor_list_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="ulong_sublist" type="ulong_sublist_t"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ulong_sublist_t">
		<xs:sequence>
			<xs:element name="JRE_job_number" type="xs:unsignedInt"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JB_shell_list_t">
		<xs:sequence >
			<xs:element name="element" type="JB_shell_list_element_t"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JB_shell_list_element_t">
		<xs:sequence >
			<xs:element name="PN_path" type="xs:string"/>
			<xs:element name="PN_host" type="xs:string"/>
			<xs:element name="PN_file_host" type="xs:string" minOccurs="0" />
			<xs:element name="PN_file_staging" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JB_env_list_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="element" type="JB_env_element_t"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JB_env_element_t">
		<xs:sequence>
			<xs:element name="VA_variable" type="xs:string"/>
			<xs:element name="VA_value" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JB_hard_queue_list_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="destin_ident_list" type="destin_ident_list_t"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="destin_ident_list_t">
		<xs:sequence>
			<xs:element name="QR_name" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JB_mail_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="element" type="JB_mail_element_t"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="JB_mail_element_t">
		<xs:sequence>
			<xs:element name="MR_user" type="xs:string"/>
			<xs:element name="MR_host" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JB_ja_structure_t">
		<xs:sequence>
			<xs:element name="task_id_range" type="JB_ja_structure_element_t"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="JB_hja_structure_t">
		<xs:sequence>
			<xs:element name="element" type="JB_ja_structure_element_t"/>
		</xs:sequence>
	</xs:complexType>

	
	<xs:complexType name="JB_ja_structure_element_t">
		<xs:sequence >
			<xs:element name="RN_min" type="xs:unsignedInt"/>
			<xs:element name="RN_max" type="xs:unsignedInt"/>
			<xs:element name="RN_step" type="xs:unsignedInt"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JB_task_id_range_t">
		<xs:sequence>
			<xs:element name="RN_min" type="xs:unsignedInt"/>
			<xs:element name="RN_max" type="xs:unsignedInt"/>
			<xs:element name="RN_step" type="xs:unsignedInt"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ======================================= -->
	<!-- qstat -j wc_job                         -->

	<xs:complexType name="ja_task_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="ulong_sublist" type="ja_task_element_t"/>
		</xs:sequence>
	</xs:complexType>

	<!-- =========== data types ================== -->
	
    <xs:complexType name="ja_task_element_t">
		<xs:sequence>
			<xs:element name="JAT_task_number" type="xs:unsignedInt"/>
			<xs:element name="JAT_status" type="xs:unsignedInt"/>
			<xs:element name="JAT_start_time" type="xs:unsignedInt"/>
			<xs:element name="JAT_end_time" type="xs:unsignedInt"/>
			<xs:element name="JAT_hold" type="xs:unsignedInt"/>
			<xs:element name="JAT_job_restarted" type="xs:unsignedInt"/>
			<xs:element name="JAT_granted_destin_identifier_list" type="JAT_granted_destin_identifier_list_t" minOccurs="0" />
			<xs:element name="JAT_master_queue" type="xs:string" minOccurs="0"/>
			<xs:element name="JAT_state" type="xs:unsignedInt"/>
			<xs:element name="JAT_pvm_ckpt_pid" type="xs:unsignedInt"/>
			<xs:element name="JAT_pending_signal" type="xs:unsignedInt"/>
			<xs:element name="JAT_pending_signal_delivery_time" type="xs:unsignedInt"/>
			<xs:element name="JAT_pid" type="xs:unsignedInt"/>
			<xs:element name="JAT_usage_list" type="JAT_usage_list_t" minOccurs="0"/>
			<xs:element name="JAT_scaled_usage_list" type="JAT_scaled_usage_list_t" minOccurs="0"/>
			<xs:element name="JAT_fshare" type="xs:unsignedInt"/>
			<xs:element name="JAT_tix" type="xs:float"/>
			<xs:element name="JAT_oticket" type="xs:float"/>
			<xs:element name="JAT_fticket" type="xs:float"/>
			<xs:element name="JAT_sticket" type="xs:float"/>
			<xs:element name="JAT_share" type="xs:float"/>
			<xs:element name="JAT_suitable" type="xs:unsignedInt"/>
			<xs:element name="JAT_pe_object" type="xs:string"/>
			<xs:element name="JAT_next_pe_task_id" type="xs:unsignedInt"/>
			<xs:element name="JAT_stop_initiate_time" type="xs:unsignedInt"/>
			<xs:element name="JAT_prio" type="xs:float"/>
			<xs:element name="JAT_ntix" type="xs:float"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JAT_granted_destin_identifier_list_t">
		<xs:sequence >
			<xs:element name="element" type="JAT_granted_destin_identifier_list_element_t"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JAT_granted_destin_identifier_list_element_t">
		<xs:sequence>
			<xs:element name="JG_qname" type="xs:string"/>
			<xs:element name="JG_qversion" type="xs:string"/>
			<xs:element name="JG_qhostname" type="xs:string"/>
			<xs:element name="JG_slots" type="xs:unsignedInt"/>
			<xs:element name="JG_queue" type="xs:string"/>
			<xs:element name="JG_tag_slave_job" type="xs:unsignedInt"/>
			<xs:element name="JG_task_id_range" type="xs:unsignedInt"/>
			<xs:element name="JG_ticket" type="xs:float"/>
			<xs:element name="JG_oticket" type="xs:float"/>
			<xs:element name="JG_fticket" type="xs:float"/>
			<xs:element name="JG_sticket" type="xs:float"/>
			<xs:element name="JG_jcoticket" type="xs:float"/>
			<xs:element name="JG_jcfticket" type="xs:float"/>
			<xs:element name="JG_processors" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JAT_usage_list_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="element" type="JAT_usage_list_element_t"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JAT_usage_list_element_t">
		<xs:sequence>
			<xs:element name="UA_name" type="xs:string"/>
			<xs:element name="UA_value" type="xs:float"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JAT_scaled_usage_list_t">
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="element" type="JAT_scaled_usage_list_element_t"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="JAT_scaled_usage_list_element_t">
		<xs:sequence>
			<xs:element name="UA_name" type="xs:string"/>
			<xs:element name="UA_value" type="xs:float"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ======================================= -->
	<!-- qstat -j wc_job                         -->
	<!-- qstat -j                                -->

	<xs:complexType name="message_t">
         <xs:annotation>
            <xs:documentation>qstat -j output</xs:documentation>
         </xs:annotation>
         <xs:sequence>
            <xs:element name="qmaster_response">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="SME_global_message_list" type="gmessage_list_t" minOccurs="0" maxOccurs="1"/>
                  </xs:sequence>
               </xs:complexType> 
            </xs:element>
         </xs:sequence>
      </xs:complexType>   

<!-- ======================================= -->
<!-- qstat -j                                -->

   <xs:complexType name="gmessage_list_t">
         <xs:sequence>
            <xs:element name="element" minOccurs="0" maxOccurs="unbounded">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="MES_message_number" type="xs:positiveInteger"/>
                     <xs:element name="MES_message" type="xs:string"/>
                  </xs:sequence>
               </xs:complexType> 
            </xs:element>
         </xs:sequence>
      </xs:complexType>   
</xs:schema>
