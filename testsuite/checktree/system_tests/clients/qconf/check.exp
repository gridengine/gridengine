proc qconf_addqueues {} {
   global CHECK_PRODUCT_ROOT CHECK_ARCH CHECK_HOST queue_names CHECK_OUTPUT

   set q_param(load_thresholds)       "np_load_avg=7.00"
 
   set count 0
   foreach queue $queue_names {
      if { $count > 50 } {
         puts $CHECK_OUTPUT "adding queue \"$queue\" with vi"
         add_queue $queue $CHECK_HOST q_param
         set count 0
      } else {
         puts $CHECK_OUTPUT "adding queue \"$queue\" with file"
         add_queue $queue $CHECK_HOST q_param 1
      }
      incr count 1
   }

   set_error 0 "ok"
}



