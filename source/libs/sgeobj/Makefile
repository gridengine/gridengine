	$(CC) $(CFLAGS) -c $(SGEOBJLIB_DIR)/test_sge_object.c

test_sge_range.o: $(SGEOBJLIB_DIR)/test_sge_range.c
	$(CC) $(CFLAGS) -c $(SGEOBJLIB_DIR)/test_sge_range.c

