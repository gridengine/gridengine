proc qconf_de_check {} {
   global CHECK_OUTPUT CHECK_PRODUCT_TYPE CHECK_HOST CHECK_USER CHECK env ts_config
   puts $CHECK_OUTPUT "testing qconf -de ..."

   # Setup
   set execd [lindex $ts_config(execd_hosts) 0]

   set catch_result [ catch {  eval exec "$ts_config(product_root)/bin/$arch/qconf -se $execd | grep -v load_values | grep -v processors > /tmp/$execd" } result ]
   
   if { $catch_result != 0 } {
      add_proc_error "qconf_de_check" "-1" "could not store execd config"
   }

   set catch_result [ catch {  eval exec "$ts_config(product_root)/bin/$arch/qconf -dattr hostgroup hostlist $execd @allhosts" } result ]
   
   if { $catch_result != 0 } {
      add_proc_error "qconf_de_check" "-1" "could not remove host from hostgroup @allhosts"
   }

   # - Disable line breaks in qconf so that the grep can work
   set env(SGE_SINGLE_LINE) 1

   # Test
   set catch_result [ catch {  eval exec "$ts_config(product_root)/bin/$arch/qconf" "-de" "$execd" } result ]
   
   if { $catch_result != 0 } {
      add_proc_error "qconf_de_check" "-1" "could not delete live execd"
   }

   # Cleanup
   unset env(SGE_SINGLE_LINE)

   set catch_result [ catch {  eval exec "$ts_config(product_root)/bin/$arch/qconf -Ae /tmp/$execd" } result ]
   
   if { $catch_result != 0 } {
      add_proc_error "qconf_de_check" "-1" "could not create exec host"
   }

   set catch_result [ catch {  eval exec "$ts_config(product_root)/bin/$arch/qconf -aattr hostgroup hostlist $execd @allhosts" } result ]
   
   if { $catch_result != 0 } {
      add_proc_error "qconf_de_check" "-1" "could not add host to hostgroup @allhosts"
   }

   set_error 0 "ok"
}
