CLIENTS_COMMON_DIR = $(SDIR)/clients/common

CLIENTS_COMMON_SRC = qstat_printing.c \
                     qstat_util.c \
                     read_defaults.c \
                     show_job.c \
                     write_job_defaults.c

CLIENTS_COMMON_OBJS = $(CLIENTS_COMMON_SRC:.c=.o)

###############################################################################
# global rules: all, clean, depend, messages
###############################################################################
clients_common: $(CLIENTS_COMMON_OBJS)

clients_common_clean:
	$(RM) $(CLIENTS_COMMON_OBJS)

clients_common_depend:
	$(DEPEND) -f$(CLIENTS_COMMON_DIR)/common_dependencies $(CLIENTS_COMMON_DIR)/*.c

clients_common_messages:
	@echo Creating $@
	@for i in `ls $(CLIENTS_COMMON_DIR)/*.c`; do \
		$(CC) $(CFLAGS) -DEXTRACT_MESSAGES -E $$i | $(XGETTEXT) - ;\
	done


###############################################################################
# include dependencies for object files generated by sge_depend
###############################################################################
include $(CLIENTS_COMMON_DIR)/common_dependencies

###############################################################################
# rules to build object codes
###############################################################################
qstat_printing.o:     $(CLIENTS_COMMON_DIR)/qstat_printing.c
	   $(CC) $(CFLAGS) -c $(CLIENTS_COMMON_DIR)/qstat_printing.c

qstat_util.o:     $(CLIENTS_COMMON_DIR)/qstat_util.c
	   $(CC) $(CFLAGS) -c $(CLIENTS_COMMON_DIR)/qstat_util.c

read_defaults.o : $(CLIENTS_COMMON_DIR)/read_defaults.c
		$(CC) $(CFLAGS) -c $(CLIENTS_COMMON_DIR)/read_defaults.c

show_job.o : $(CLIENTS_COMMON_DIR)/show_job.c
		$(CC) $(CFLAGS) -c $(CLIENTS_COMMON_DIR)/show_job.c

write_job_defaults.o:	$(CLIENTS_COMMON_DIR)/write_job_defaults.c
		$(CC) $(CFLAGS) -c $(CLIENTS_COMMON_DIR)/write_job_defaults.c

